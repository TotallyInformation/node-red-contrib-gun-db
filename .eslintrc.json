{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "script",
        "ecmaFeatures": {
            "impliedStrict": true,
            "globalReturn": false,
            "jsx": false
        }
    },
    "env": {
        "node": true,
        "shared-node-browser": true,
        "browser": true,
        "es6": true,
        "es2017": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "never"
        ],
        "comma-dangle": "off",
        "no-unused-vars": "warn"
    }
}